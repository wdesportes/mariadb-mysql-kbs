      "scope": [
        "global"
      ],
      "dynamic": true,
      "type": "integer",
      "default": "1",
      "range": {
        "from": 1,
        "to": 65535
      }
    },
    {
      "id": "sysvar_rpl_semi_sync_master_wait_no_slave",
      "name": "rpl_semi_sync_master_wait_no_slave",
      "scope": [
        "global"
      ],
      "dynamic": true,
      "type": "boolean",
      "default": "on"
    },
    {
