      "name": "auto_increment_increment",
      "scope": [
        "global",
        "session"
      ],
      "dynamic": true,
      "type": "integer",
      "default": "1",
      "range": {
        "from": 1,
        "to": 65535
      }
    },
    {
      "id": "sysvar_auto_increment_offset",
      "name": "auto_increment_offset",
      "scope": [
        "global",
        "session"
      ],
      "dynamic": true,
      "type": "integer",
      "default": "1",
      "range": {
        "from": 1,
        "to": 65535
      }
    },
    {
      "id": "sysvar_rpl_semi_sync_master_enabled",
      "name": "rpl_semi_sync_master_enabled",
      "scope": [
        "global"
      ],
      "dynamic": true,
      "type": "boolean",
      "default": "off"
    },
    {
      "id": "sysvar_rpl_semi_sync_master_timeout",
      "name": "rpl_semi_sync_master_timeout",
      "scope": [
        "global"
      ],
      "dynamic": true,
      "type": "integer",
      "default": "10000"
    },
    {
